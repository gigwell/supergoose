{
  "name": "supergoose",
  "description": "Mongoose plugin for simple addons like findOrCreate",
  "version": "0.2.6",
  "keywords": [
    "supergoose",
    "mongodb",
    "mongoose",
    "plugin",
    "findOrCreate",
    "relationship",
    "parent",
    "child",
    "many to many",
    "one to one",
    "one to many",
    "many to one"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/jamplify/supergoose"
  },
  "main": "./lib/supergoose",
  "dependencies": {
    "mongoose": ">= 2.6.5",
    "underscore": ">= 1.3.3",
    "underscore.string": "3.2.2",
    "inflect": "~0.3.0",
    "async": "~0.2.10"
  },
  "devDependencies": {
    "should": ">= 0.2.1",
    "mocha": ">= 1.13.0"
  },
  "scripts": {
    "test": "mocha -u bdd -R spec -c ./test/*test.js"
  },
  "license": "MIT",
  "engine": {
    "node": ">= 0.6"
  },
  "author": {
    "name": "Matt Roman",
    "email": "matt@jamplify.com"
  },
  "contributors": [
    {
      "name": "JP Erkelens",
      "email": "jp@jamplify.com"
    }
  ],
  "jshintConfig": {
    "eqeqeq": true,
    "indent": 2,
    "esnext": true,
    "asi": true,
    "maxlen": 80,
    "singleGroups": true,
    "unused": true
  }
}
